LocalScript31.Parent = Frame29
table.insert(cors,sandbox(LocalScript31,function()
local bar = script.Parent

local done = script.Parent.Parent.Loaded.Value

while wait(1) do
	if done == "No" then
		bar.Size = bar.Size + UDim2.new(0.1, 0, 0, 0)
		end
end
end))
