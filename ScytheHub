LocalScript6.Parent = TextButton5
table.insert(cors,sandbox(LocalScript6,function()
script.Parent.MouseButton1Click:Connect(function()
	if script.Parent.Parent.MainArea.Visible == false then
		script.Parent.Parent.MainArea.Visible = true
		script.Parent.Text = "-"
		script.Parent.Parent.MainArea:TweenSize(UDim2.new(0, 200, 0,400), 'Out', 'Sine', '1')
	else
		script.Parent.Text = "+"
		script.Parent.Parent.MainArea:TweenSize(UDim2.new(0, 200, 0), 'Out', 'Sine', '1')
		wait(1)
